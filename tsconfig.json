{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "lib": ["esnext", "DOM"],
        "paths": {
            "$lib": ["src/lib"],
            "$lib/*": ["src/lib/*"],
            "$components": ["src/components"],
            "$components/*": ["src/components/*"],
            "$containers": ["src/containers"],
            "$containers/*": ["src/containers/*"]
        }
    }
}
